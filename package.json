{
  "name": "gametest",
  "version": "1.0.0",
  "description": "Grand strategy economic simulation prototype",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "simulate": "ts-node src/index.ts",
    "play": "npx ts-node src/play.ts",
    "dashboard": "ts-node src/ui.ts",
    "dashboard:prepare": "npm run dashboard && node scripts/sync-dashboard-data.js",
    "dashboard:dev": "npm run dashboard:prepare && vite --config dashboard-app/vite.config.mts",
    "dashboard:build:spa": "npm run dashboard:prepare && vite build --config dashboard-app/vite.config.mts",
    "dashboard:preview": "vite preview --config dashboard-app/vite.config.mts",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^5.3.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
